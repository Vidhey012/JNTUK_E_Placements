                                <b><%= data.course%> - <%= data.branch%>&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;CGPA : <%= data.cgpa%> / 10</b>
                                <i align="right" style="position: absolute; right: 10px;">20<%= year %>-20<%= btechyear %></i>
                            </p>
                        </div>
                    </div>
                    <div class="row card" style="margin-top:10px;">
                        <div class="col-12">
                            <h6 style="text-transform: uppercase; margin-bottom: 0px;  margin-top: 5px;"><b><%= data.intercollege%></b></h6>
                            <p style="margin-top:0px; margin-bottom: 5px;">
                                <b>12<sup>th</sup>&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;CGPA : <%= data.gpa12%> / 10</b>
                                <i align="right" style="position: absolute; right: 10px;">20<%= year %></i>
                            </p>
                        </div>
                    </div>
                    <div class="row card" style="margin-top:10px;">
                        <div class="col-12">
                            <h6 style="text-transform: uppercase; margin-bottom: 0px;  margin-top: 5px;"><b><%= data.schoool%></b></h6>
                            <p style="margin-top:0px; margin-bottom: 5px;">
                                <b>10<sup>th</sup>&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;CGPA : <%= data.gpa10%> / 10</b>
                                <i align="right" style="position: absolute; right: 10px;">20<%= tenthyear %></i>
                            </p>
                        </div>
                    </div>

                    <div class="row" style="margin-top:10px;">
                        <div class="col-12 " style="background-color: #0096dd; color: white; text-transform: uppercase;">
                            <b>TECHNICAL &nbsp;&nbsp;SKILLS</b>
                        </div>
                    </div>

                    <% var skillsArr=data.skills.split(","); %>
                        <div class="row" style="margin-top:10px;">
                            <% for (var i=0; i < skillsArr.length; i++) { %>
                                <div class="col" align="center" style="background-color:#eeeeeb ; border-radius: 10px;  font-size: 0.83em;">
                                    <%= skillsArr[i] %>
                                </div>&nbsp;&nbsp;
                                <% if((i+1)%5==0 && i!=skillsArr.length-1){ %>
                        </div>
                        <div class="row" style="margin-top:10px;">
                            <% } %>
                                <% } %>
                        </div>

                        <div class="row" style="margin-top:10px;">
                            <div class="col-12 " style="background-color: #0096dd; color: white; text-transform: uppercase;">
                                <b>Certifications</b>
                            </div>
                        </div>
                        <ul style="font-size: 0.83em; margin-top: 10px;">
                            <% var certifiArr=data.certifications.split(","); %>
                                <% for (var i=0; i < certifiArr.length; i++) { %>
                                    <li style="font-size: 1.2em;">
                                        <%= certifiArr[i] %>
                                    </li>
                                    <% } %>
                        </ul>

                        <div class="row" style="margin-top:10px;">
                            <div class="col-12 " style="background-color: #0096dd; color: white; text-transform: uppercase;">
                                <b>PROJECTS</b>
                            </div>
                        </div>
                        <% var proArr=data.projects.split(","); %>
                            <% for (var i=0; i < proArr.length; i++) { %>
                                <div class="row" style="margin-top:10px;">
                                    <div class="col" align="left" style="background-color:#eeeeeb ; border-radius: 10px;  font-size: 1.0em; padding-bottom: 3px;">
                                        <%= proArr[i] %>
                                    </div>
                                </div>
                                <% } %>

                                    <div class="row" style="margin-top:10px;">
                                        <div class="col-12 " style="background-color: #0096dd; color: white; text-transform: uppercase;">
                                            <b>Achievements &nbsp;&nbsp;/ Awards</b>
                                        </div>
                                    </div>
                                    <ul style="font-size: 0.83em; margin-top: 10px;">
                                        <% var achiArr=data.achievements.split(","); %>
                                            <% for (var i=0; i < achiArr.length; i++) { %>
                                                <li style="font-size: 1.2em;">
                                                    <%= achiArr[i] %>
                                                </li>
                                                <% } %>
                                    </ul>

                                    <div class="row" style="margin-top:10px;">
                                        <div class="col-12 " style="background-color: #0096dd; color: white; text-transform: uppercase;">
                                            <b>PERSONAL&nbsp;&nbsp; DETAILS</b>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-6"><b>Date of Birth : </b>
                                            <%= data.dob%><br/><b>Gender : </b>
                                                <%= data.gender%>
                                        </div>
                                        <div class="col-6"><b>Phone No : </b>
                                            <%= data.phone%><br/><b>Email : </b>
                                                <%= data.email%>
                                        </div>
                                        <div class="col-12"><b>Current Address : </b>
                                            <%= data.address%>
                                        </div>
                                    </div>
    </div>



