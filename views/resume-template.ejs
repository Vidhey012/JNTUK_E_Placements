        window.jsPDF = window.jspdf.jsPDF;

        function Convert_HTML_To_PDF() {
            var width = document.getElementById('resume').clientWidth;
            var height = document.getElementById('resume').clientHeight;
            var name = (document.getElementById("username").innerHTML).trim();
            var doc = new jsPDF('p', 'pt', [width, height]);
            var pageCount = doc.internal.getNumberOfPages();
            var elementHTML = document.querySelector("#resume");
            doc.html(elementHTML, {
                callback: function(doc) {
                    doc.deletePage(pageCount + 1);
                    doc.save(name + '_resume.pdf');
                }
            });
        }
